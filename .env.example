# Copy this file to .env and fill in your actual values.
# DO NOT commit the .env file - it is listed in .gitignore.

# AI Pipeline token for external AI service
AIPIPE_TOKEN=your_aipipe_token_here

# AWS credentials
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here

# PostgreSQL connection string
# Format: postgres://user:password@host:port/dbname
DATABASE_URL=postgres://user:password@localhost:5432/dbname

# API secret key for signing requests
API_SECRET=your_api_secret_here

# JWT secret key used to sign authentication tokens (use a long random string)
JWT_SECRET=your_jwt_secret_here

# Flask settings
PORT=5000
